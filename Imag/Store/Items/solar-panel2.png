<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 200" width="200" height="200">
  <!-- Solar Panel Base - Blue/Dark Blue Gradient -->
  <defs>
    <linearGradient id="panelGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3a8a;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2563eb;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e40af;stop-opacity:1" />
    </linearGradient>
    
    <!-- Shine/Reflection Gradient -->
    <linearGradient id="shine" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#60a5fa;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:0" />
    </linearGradient>
    
    <!-- Sun Glow -->
    <radialGradient id="sunGlow">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#f59e0b;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#d97706;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background Glow -->
  <circle cx="100" cy="50" r="45" fill="url(#sunGlow)" opacity="0.3"/>
  
  <!-- Sun -->
  <circle cx="100" cy="50" r="25" fill="#fbbf24" stroke="#f59e0b" stroke-width="3"/>
  
  <!-- Sun rays -->
  <g stroke="#fbbf24" stroke-width="4" stroke-linecap="round" opacity="0.8">
    <line x1="100" y1="15" x2="100" y2="5"/>
    <line x1="100" y1="85" x2="100" y2="95"/>
    <line x1="65" y1="50" x2="55" y2="50"/>
    <line x1="135" y1="50" x2="145" y2="50"/>
    <line x1="75" y1="25" x2="68" y2="18"/>
    <line x1="125" y1="75" x2="132" y2="82"/>
    <line x1="125" y1="25" x2="132" y2="18"/>
    <line x1="75" y1="75" x2="68" y2="82"/>
  </g>
  
  <!-- Solar Panel Frame -->
  <rect x="35" y="95" width="130" height="95" rx="8" fill="#374151" stroke="#1f2937" stroke-width="4"/>
  
  <!-- Solar Panel Surface -->
  <rect x="40" y="100" width="120" height="85" rx="5" fill="url(#panelGrad)" stroke="#1e293b" stroke-width="2"/>
  
  <!-- Grid Lines (photovoltaic cells) -->
  <g stroke="#1e293b" stroke-width="2" opacity="0.6">
    <!-- Vertical lines -->
    <line x1="80" y1="100" x2="80" y2="185"/>
    <line x1="120" y1="100" x2="120" y2="185"/>
    <!-- Horizontal lines -->
    <line x1="40" y1="128" x2="160" y2="128"/>
    <line x1="40" y1="156" x2="160" y2="156"/>
  </g>
  
  <!-- Shine/Reflection -->
  <rect x="45" y="105" width="50" height="35" rx="3" fill="url(#shine)" opacity="0.4"/>
  
  <!-- Energy Bolts -->
  <g fill="#fbbf24" opacity="0.9">
    <path d="M 95 125 L 100 135 L 97 135 L 102 145 L 94 135 L 97 135 Z"/>
    <path d="M 110 140 L 115 150 L 112 150 L 117 160 L 109 150 L 112 150 Z"/>
  </g>
  
  <!-- Support Stand -->
  <rect x="85" y="185" width="30" height="8" rx="2" fill="#4b5563" stroke="#374151" stroke-width="2"/>
  
  <!-- Connection Point -->
  <circle cx="100" cy="190" r="5" fill="#6b7280" stroke="#374151" stroke-width="2"/>
</svg>
